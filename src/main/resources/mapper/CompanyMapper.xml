<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.CompanyMapper">
    <select id="queryAll" resultType="com.example.demo.bean.Company">
        select * from mybatis.company
    </select>

    <insert id="insert" parameterType="com.example.demo.bean.Company">
        insert into mybatis.company values(#{id},#{createTime},#{name})
    </insert>
</mapper>